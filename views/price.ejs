        <div class="row my-2 g-3 gx-lg-4 pb-3">
          <div class="col-12">
            <div class="mainchart px-3 px-md-4 py-3 py-lg-4 ">
              <div class="d-flex justify-content-between flex-wrap gap-4">
                <h5 class="mb-0">Crytocurrency List</h5>
                <div class="d-flex flex-wrap flex-md-nowrap gap-4 align-items-center">
                  <form class="d-flex align-items-center search-form  px-2 flex-shrink-0">
                    <input type="text" placeholder="Search...">
                    <span class="material-symbols-outlined"> search </span>
                  </form>
                  <select class="form-select price-select">
                    <option value="all">All Time</option>
                    <option value="month">This month</option>
                    <option value="week">This week</option>
                  </select>
                </div>
              </div>
              <div class="pb-2 pt-3 price-table">
                <table>
                  <thead>
                    <tr>
                      <th class="fw-bold"></th>
                      <th class="fw-bold">#</th>
                      <th class="fw-bold">Name</th>
                      <th class="fw-bold">Price</th>
                      <th class="fw-bold">24h %</th>
                      <th class="fw-bold">Market Cap</th>
                      <th class="fw-bold">Volume</th>
                      <th class="fw-bold">Trend</th>
                      <th></th>
                    </tr>
                  </thead>
                  <tbody>

                    <% coinsData.forEach((coin, index) => { %>
                      <tr>
                        <td><span class="material-symbols-outlined star">
                            star
                          </span></td>
                        <td><%= index+1 %> </td>
                        <td class="d-flex align-items-center gap-2">
                         
                          <img src="<%= coin.image %>" style="height:40px;width:40px" alt="">
                          <%= coin.name %>
                        </td>
                        <td>$ <%= coin.current_price %></td>
                        <% let className = coin.price_change_percentage_24h < 0 ? 'secondary' : 'primary'; %>
                        <td class="<%= className %>"><%= coin.price_change_percentage_24h %>%</td>
                        <td>$<%=coin.market_cap%></td></td>
                        <td>$<%=coin.total_volume%></td>
                        <% let trendImage = coin.price_change_percentage_24h < 0 ? 'assets/img/chart_red_1.png' : 'assets/img/chart_green.png'; %>
                        <td><img src="<%= trendImage %>" alt=""></td>
                        <td><a href="<%= `/exchange?id=${coin.id}` %>" class="outline-btn">View</a></td>
                      </tr>
                    <% }); %>

                  </tbody>
                </table>
              </div>
            </div>
